{
    "name": "parse-server-typescript",
    "version": "1.0.0",
    "description": "Parse Server using Typescript",
    "main": "./dist/src/index.js",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dodi-smart/parse-server-typescript.git"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "graphql": "15.5",
        "parse-dashboard": "parse-community/parse-dashboard#master",
        "parse-server": "dodi-smart/parse-server#4.10.4-dodi.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.24",
        "@types/node": "~16.4.13",
        "@types/parse": "~2.18.8",
        "@types/throng": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.24.0",
        "jest": "^27.0.6",
        "nyc": "^15.1.0",
        "parse": "~3.3.0",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5"
    },
    "resolutions": {
        "graphql": "15.5"
    },
    "scripts": {
        "dev": "TS_NODE_FILES=true node --require ts-node/register --trace-warnings ./src/index.ts",
        "lint": "eslint src --ext .ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "unit": "jest --config tests/unit/jest.config.js",
        "integration": "PARSE_SILENT=true ./tests/run_integration_tests.sh",
        "test": "yarn lint && yarn build && yarn unit && yarn integration",
        "coverage": "nyc --silent yarn integration && nyc report"
    },
    "engines": {
        "node": "14.18",
        "yarn": "1.22"
    }
}
