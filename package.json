{
    "name": "parse-server-typescript",
    "version": "1.0.0",
    "description": "Parse Server using Typescript",
    "main": "./dist/src/index.js",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/L3K0V/parse-server-typescript.git"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql": "15.4.0",
        "parse-dashboard": "~2.1.0",
        "parse-server": "~4.5.0",
        "snyk": "^1.428.2",
        "throng": "^5.0.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.9",
        "@types/jest": "^26.0.19",
        "@types/node": "~14.14.16",
        "@types/parse": "~2.18.1",
        "@types/throng": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.16.0",
        "eslint-plugin-import": "^2.22.1",
        "jest": "^26.6.3",
        "nyc": "^15.1.0",
        "parse": "~2.19.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.12",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "resolutions": {
        "graphql": "15.4.0"
    },
    "scripts": {
        "dev": "TS_NODE_FILES=true node --require ts-node/register --trace-warnings ./src/index.ts",
        "lint": "eslint src --ext .ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "unit": "jest --config tests/unit/jest.config.js",
        "integration": "PARSE_SILENT=true ./tests/run_integration_tests.sh",
        "test": "yarn lint && yarn build && yarn unit && yarn integration",
        "coverage": "nyc --silent yarn integration && nyc report",
        "snyk-protect": "snyk protect",
        "prepare": "yarn snyk-protect"
    },
    "engines": {
        "node": "14.15.3",
        "npm": "6.14.9"
    },
    "snyk": true
}