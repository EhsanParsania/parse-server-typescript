{
    "name": "parse-server-typescript",
    "version": "1.0.0",
    "description": "Parse Server using Typescript",
    "main": "./dist/src/index.js",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dodi-smart/parse-server-typescript.git"
    },
    "scripts": {
        "dev": "yarn build && nodemon",
        "dev:debug": "nodemon --inspect",
        "lint": "eslint ./ --ext .ts,.js",
        "build": "tsc",
        "start": "node dist/src/index.js",
        "unit": "jest --config tests/unit/jest.config.js",
        "integration": "PARSE_SILENT=true ./tests/run_integration_tests.sh",
        "test": "yarn lint && yarn build && yarn unit && yarn integration",
        "coverage": "nyc --silent yarn integration && nyc report"
    },
    "dependencies": {
        "dotenv": "16.0.3",
        "express": "4.18.2",
        "graphql": "16.6.0",
        "module-alias": "2.2.2",
        "parse-dashboard": "5.0.0",
        "parse-server": "5.4.0"
    },
    "devDependencies": {
        "@types/express": "4.17.16",
        "@types/jest": "29.4.0",
        "@types/node": "16.18.11",
        "@types/parse": "3.0.2",
        "@typescript-eslint/eslint-plugin": "5.49.0",
        "@typescript-eslint/parser": "5.49.0",
        "babel-eslint": "10.1.0",
        "babel-watch": "7.7.2",
        "eslint": "8.32.0",
        "eslint-plugin-import": "2.27.5",
        "jest": "29.4.0",
        "nodemon": "2.0.20",
        "nyc": "15.1.0",
        "ts-jest": "29.0.5",
        "ts-loader": "9.4.2",
        "ts-node": "10.9.1",
        "typescript": "4.9.4"
    },
    "resolutions": {
        "graphql": "16.5.0"
    },
    "engines": {
        "node": "16",
        "npm": "please-use-yarn",
        "yarn": "1.22"
    },
    "_moduleAliases": {
        "@": "dist/src"
    }
}
