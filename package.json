{
    "name": "parse-server-typescript",
    "version": "1.0.0",
    "description": "Parse Server using Typescript",
    "main": "./dist/src/index.js",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/L3K0V/parse-server-typescript.git"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "graphql": "15.5.1",
        "parse-dashboard": "~2.1.0",
        "parse-server": "~4.5.0",
        "throng": "^5.0.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.12",
        "@types/jest": "^26.0.23",
        "@types/node": "~16.0.0",
        "@types/parse": "~2.18.6",
        "@types/throng": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.30.0",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.0.6",
        "nyc": "^15.1.0",
        "parse": "~3.2.0",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5"
    },
    "resolutions": {
        "graphql": "15.4.0"
    },
    "scripts": {
        "dev": "TS_NODE_FILES=true node --require ts-node/register --trace-warnings ./src/index.ts",
        "lint": "eslint src --ext .ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "unit": "jest --config tests/unit/jest.config.js",
        "integration": "PARSE_SILENT=true ./tests/run_integration_tests.sh",
        "test": "yarn lint && yarn build && yarn unit && yarn integration",
        "coverage": "nyc --silent yarn integration && nyc report",
        "snyk-protect": "snyk protect",
        "prepare": "yarn snyk-protect"
    },
    "engines": {
        "node": "14.15.4",
        "yarn": "1.22.10"
    }
}
