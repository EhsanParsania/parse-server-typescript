{
    "name": "parse-server-typescript",
    "version": "1.0.0",
    "description": "Parse Server using Typescript",
    "main": "./dist/src/index.js",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dodi-smart/parse-server-typescript.git"
    },
    "dependencies": {
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "graphql": "16.3.0",
        "parse-dashboard": "^4.0.0",
        "parse-server": "5.0.0-alpha.24"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "~16.4.13",
        "@types/parse": "~2.18.13",
        "@types/throng": "^5.0.3",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "^2.25.4",
        "jest": "^27.5.1",
        "nyc": "^15.1.0",
        "parse": "~3.4.1",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "resolutions": {
        "graphql": "16.3.0"
    },
    "scripts": {
        "dev": "TS_NODE_FILES=true node --require ts-node/register --trace-warnings ./src/index.ts",
        "lint": "eslint src --ext .ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "unit": "jest --config tests/unit/jest.config.js",
        "integration": "PARSE_SILENT=true ./tests/run_integration_tests.sh",
        "test": "yarn lint && yarn build && yarn unit && yarn integration",
        "coverage": "nyc --silent yarn integration && nyc report"
    },
    "engines": {
        "node": "16",
        "yarn": "1.22"
    }
}
